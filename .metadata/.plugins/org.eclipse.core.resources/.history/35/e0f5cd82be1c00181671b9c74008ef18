package finalproj;

public class Food 
{
	private int time;
	private char ingredients;
	public char getIngredients() {
		return ingredients;
	}
	public void setIngredients(char ingredients) {
		this.ingredients = ingredients;
	}
	public int getTime() {
		return time;
	}
	public void setTime(int time) {
		this.time = time;
	}

}
